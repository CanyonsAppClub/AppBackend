{
    "calendar":{
        "events":[
            {% for event in events %}
            {
                    "id":{{ event.id }},
                    "start_time":"{{ event.start_date }}",
                    "end_time":"{{ event.end_date }}",
                    "location":"{{ event.location }}",
                    "location_id":"{{ event.location.id }}",
                    "title":"{{ event.event_title }}",
                    "subtitle":"{{ event.event_subtitle }}"
            }
            {% if not forloop.last %},{% endif %}
            {% endfor %}
        ]
    }
}